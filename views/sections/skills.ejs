<section id="skills">
    <div class="container-fluid">
        <div>
            <h2 class="skills-heading">My Skills</h2>
        </div>
        <div class="skills-container">
            <% let i = 0; %>
                <% while(i < skills.length){ %>
                    <% let skill = skills[i]; %>
                    <button type="button" class="btn btn-secondary"><%=skill%></button>
                <% i++; %>    
                <% } %>
        </div>
    </div>
</section>